<Project Sdk="Microsoft.NET.Sdk">

   <PropertyGroup>
      <TargetFramework>net6.0</TargetFramework>
      <ImplicitUsings>enable</ImplicitUsings>
      <Nullable>enable</Nullable>
      <GeneratePackageOnBuild>True</GeneratePackageOnBuild>
      <Title>Command Line Arguments Parser</Title>
      <Company />
      <Authors>Fabian Quintero</Authors>
      <Version>1.0.2</Version>
      <Description>A simple command line arguments parser configurable in a single line</Description>
      <RepositoryUrl>https://github.com/f-quintero/Command-line-argument-parser</RepositoryUrl>
      <RepositoryType>GIT</RepositoryType>
      <PackageReleaseNotes></PackageReleaseNotes>
      <PackageLicenseFile>Licence.txt</PackageLicenseFile>
      <PackageRequireLicenseAcceptance>True</PackageRequireLicenseAcceptance>
      <PackageReadmeFile>README.md</PackageReadmeFile>
   </PropertyGroup>

   <ItemGroup>
     <None Update="Readme.md">
       <Pack>True</Pack>
       <PackagePath>\</PackagePath>
     </None>
   </ItemGroup>

   <ItemGroup>
     <None Update="Licence.txt">
       <Pack>True</Pack>
       <PackagePath>\</PackagePath>
     </None>
   </ItemGroup>

</Project>
